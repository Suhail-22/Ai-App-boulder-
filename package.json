    "monaco-editor": "^0.44.0"
  },
  "devDependencies": {
    "serve": "^14.2.1",
    "live-server": "^1.2.2",
    "vercel": "^33.0.1",
    "workbox-cli": "^7.0.0",
    "sharp": "^0.32.6"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/bolt-ai.git"
  },
  "bugs": {
