{
  "name": "bolt-ai-studio",
  "version": "2.0.0",
  "description": "منصة تطوير التطبيقات بالذكاء الاصطناعي - PWA",
  "main": "index.html",
  "scripts": {
    "start": "serve . -p 3000",
    "dev": "live-server . --port=3000",
    "build": "echo 'No build required'",
    "test": "echo 'No tests specified'",
