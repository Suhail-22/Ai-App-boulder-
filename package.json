    "deploy": "vercel --prod",
    "generate-icons": "node scripts/generate-icons.js",
    "generate-sw": "workbox generateSW workbox-config.js",
    "update": "npm run generate-icons && npm run generate-sw"
  },
  "keywords": [
    "ai",
